{
  "productName": "Tauri Deno Mobile Sample",
  "version": "0.1.0",
  "identifier": "me.bpev.tauri-mobile-sample-app",
  "build": {
    "beforeBuildCommand": "deno run -A build.ts build",
    "beforeDevCommand": "deno run -A build.ts dev",
    "devUrl": "http://0.0.0.0:3000",
    "frontendDist": "../src-www"
  },
  "app": {
    "withGlobalTauri": false,
    "security": {
      "csp": null
    },
    "windows": [
      {
        "title": "main",
        "fullscreen": false,
        "height": 800,
        "resizable": false,
        "title": "Inro",
        "width": 360
      }
    ]
  },
  "bundle": {
    "active": true,
    "category": "DeveloperTool",
    "copyright": "",
    "icon": [
      "icons/32x32.png",
      "icons/128x128.png",
      "icons/128x128@2x.png",
      "icons/icon.icns",
      "icons/icon.ico"
    ]
  }
}
